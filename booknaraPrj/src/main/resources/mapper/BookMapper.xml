<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.booknara.booknaraPrj.mapper.BookMapper">

    <!--도서 삽입 -->
    <insert id="insertBook" parameterType="java.util.List">
        INSERT IGNORE INTO book_isbn
        (isbn13, bookname, authors, publisher)
        VALUES
        <foreach collection="list" item="b" separator=",">
            (#{b.isbn13}, #{b.bookname}, #{b.authors}, #{b.publisher})
        </foreach>
    </insert>

</mapper>
